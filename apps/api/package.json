{
  "name": "@upms/api",
  "version": "0.1.0",
  "description": "UMC 9th Leo 지부의 프로젝트 팀 매칭을 위한 프로젝트 입니다.",
  "author": "Kyeoung Woon Park",
  "private": true,
  "license": "&copy; 2025 Kyeoung Woon Park. All rights reserved.",
  "scripts": {
    "build": "pnpm remove-leftovers && nest build",
    "start": "nest start",
    "dev": "pnpm remove-leftovers && cross-env NODE_ENV=development nest start --watch",
    "prod:watch": "pnpm remove-leftovers && cross-env NODE_ENV=production nest start --watch",
    "start:dev": "pnpm remove-leftovers && cross-env NODE_ENV=development nest start --watch",
    "start:debug": "pnpm remove-leftovers && cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "pnpm remove-leftovers && cross-env NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "remove-leftovers": "rimraf dist tsconfig.build.tsbuildinfo tsconfig.tsbuildinfo",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "pnpm prisma generate --schema=prisma/mongodb/schema.prisma && pnpm prisma generate --schema=prisma/postgresql/schema.prisma",
    "prisma:push": "pnpm prisma db push --schema=prisma/mongodb/schema.prisma && pnpm prisma db push --schema=prisma/postgresql/schema.prisma"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.922.0",
    "@aws-sdk/s3-request-presigner": "^3.922.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.8",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.8",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.8",
    "@nestjs/swagger": "^11.2.1",
    "@prisma/client": "6.13.0",
    "@scalar/nestjs-api-reference": "^1.0.7",
    "@sentry/nestjs": "^10.22.0",
    "@sentry/node": "^10.22.0",
    "@sentry/profiling-node": "^10.22.0",
    "@upms/shared": "workspace:*",
    "argon2": "^0.44.0",
    "axios": "^1.13.1",
    "benchmark": "^2.1.4",
    "cheerio": "^1.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "cookie-parser": "^1.4.7",
    "google-auth-library": "^10.5.0",
    "gpx-builder": "^5.6.0",
    "nanoid": "^5.1.6",
    "nest-winston": "^1.10.2",
    "nestjs-redoc": "^2.2.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "puppeteer": "^24.29.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.8",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.14.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.5",
    "@types/jest": "^29.5.14",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.18.13",
    "@types/passport-google-oauth20": "^2.0.17",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "jest": "^29.7.0",
    "lefthook": "^1.13.6",
    "prettier": "^3.6.2",
    "prisma": "6.13.0",
    "rimraf": "^6.1.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
